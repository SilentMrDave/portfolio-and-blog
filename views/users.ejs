<!DOCTYPE html>
<html>
<head>
	<title>Users</title>
	<link rel="stylesheet" type="text/css" href="css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="css/style.css">
</head>
<body>
	<%- include('partials/header') %>
	<div class="container">    
		<h1>Users</h1>    
		<table class="table">
			<tr>
				<th>Username</th>
				<th>Email</th>
			</tr>
			<% for(var i = 0; i < data.length; i++) { %>    
			<tr class="table-hover">
				<td><%=  data[i].username %> </td>
				<td><%=  data[i].email %> </td>
			</tr> 			   
			<% } %>    
		</table>
	</div>
</body>
</html>
